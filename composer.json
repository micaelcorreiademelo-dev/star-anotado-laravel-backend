{
    "name": "staranotado/backend-api",
    "type": "project",
    "description": "StarAnotado Backend API - Sistema de delivery reconstruído em código limpo",
    "keywords": ["laravel", "api", "delivery", "supabase", "postgresql", "whatsapp-bot"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "doctrine/dbal": "^3.6",
        "predis/predis": "^2.0",
        "pusher/pusher-php-server": "^7.2",
        "league/flysystem-aws-s3-v3": "^3.0",
        "intervention/image": "^2.7",
        "spatie/laravel-cors": "^1.3",
        "spatie/laravel-permission": "^5.10",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-query-builder": "^5.2",
        "barryvdh/laravel-cors": "^2.0",
        "fruitcake/laravel-cors": "^3.0",
        "laravel/horizon": "^5.15",
        "laravel/telescope": "^4.15"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0",
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "friendsofphp/php-cs-fixer": "^3.21",
        "larastan/larastan": "^2.6",
        "pestphp/pest": "^2.8",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": [
            "@php artisan test"
        ],
        "test-coverage": [
            "@php artisan test --coverage"
        ],
        "pint": [
            "./vendor/bin/pint"
        ],
        "pint-test": [
            "./vendor/bin/pint --test"
        ],
        "stan": [
            "./vendor/bin/phpstan analyse"
        ],
        "cs-fix": [
            "./vendor/bin/php-cs-fixer fix"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "fresh": [
            "@php artisan migrate:fresh --seed"
        ],
        "setup": [
            "@composer install",
            "@php artisan key:generate",
            "@php artisan storage:link",
            "@php artisan migrate:fresh --seed"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}